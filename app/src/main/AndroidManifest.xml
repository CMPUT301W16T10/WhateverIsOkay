<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.suhussai.gameshare">

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- http://stackoverflow.com/questions/1560788/how-to-check-internet-access-on-android-inetaddress-never-timeouts
     User: gar
     http://stackoverflow.com/questions/17327120/how-to-comment-a-single-line-in-xml
     User: Ryan
     Date: Thu Mar 24
     -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ViewLogIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewUserProfile" />
        <activity android:name=".ViewItem" />
        <activity android:name=".ViewItemsList" />
    </application>

</manifest>