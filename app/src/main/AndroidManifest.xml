        </activity>
        <activity android:name=".ViewUserProfile" />
        <activity android:name=".ViewBidsOnItem" />
        <activity android:name=".ViewBorrowedItems" />
        <activity android:name=".ViewItem" />
        <activity android:name=".ViewMyItems" />
    </application>

</manifest>